{
  "settingsInheritedFrom": "DimaMend/whitesource-config@main",
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "none",
    "displayMode": "diff",
    "useMendCheckNames": true
  },
  "issueSettings": {
    "minSeverityLevel": "NONE",
    "issueType": "DEPENDENCY"
  },
  "scanSettingsSAST": {
    "enableScan": "false"
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
      "config:recommended",
      "mergeConfidence:all-badges",
      "github>mend-toolkit/mend-examples//Repo-Integration/Renovate/smart-merge"
    ],
    "packageRules": [
      {
        "matchManagers": ["bazel"],
        "matchDatasources": ["maven"],
        "versioning": "maven"
      }
    ]
  }
}
